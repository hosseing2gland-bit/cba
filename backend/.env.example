NODE_ENV=development
PORT=3000
ALLOWED_ORIGINS=http://localhost:3000
BODY_LIMIT=1mb
LOG_LEVEL=info
LOG_PRETTY=true

MONGODB_URI=mongodb://localhost:27017/antidetect
MONGO_MAX_POOL_SIZE=10
MONGO_MIN_POOL_SIZE=0
MONGO_SELECTION_TIMEOUT_MS=5000
MONGO_SOCKET_TIMEOUT_MS=45000
MONGO_MAX_RETRIES=5
MONGO_RETRY_DELAY_MS=2000

JWT_ACCESS_KEY_SET=primary:access-secret-v1,backup:access-secret-v0
JWT_REFRESH_KEY_SET=primary:refresh-secret-v1,backup:refresh-secret-v0
JWT_ACTIVE_KID=primary
JWT_REFRESH_ACTIVE_KID=primary
JWT_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

ENCRYPTION_KEYS=primary:12345678901234567890123456789012,backup:abcdefabcdefabcdefabcdefabcdefab
ENCRYPTION_ACTIVE_KID=primary

AWS_ACCESS_KEY=your-aws-key
AWS_SECRET_KEY=your-aws-secret
AWS_BUCKET=antidetect-profiles
